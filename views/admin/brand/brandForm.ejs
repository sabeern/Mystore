<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no"/>
    <title>MyStore | New Brand</title>
    <!--included css file-->
    <%- include('../../partials/admin/adminCss') %>
</head>
<body class="crm_body_bg">
    <!-- navbar  -->
    <%- include('../../partials/admin/adminNavbar') %>
    <section class="main_content dashboard_part large_header_bg">
        <%- include('../../partials/admin/adminHeader') %>
    <!--Body-->
        <div class="main_content_iner ">
            <div class="container-fluid p-0">
                <div class="row justify-content-center">
                    <div class="col-12">
                        <div class="dashboard_header mb_30">
                            <div class="row">
                                <div class="col-lg-6">
                                    <div class="dashboard_header_title">
                                        <h3>New Brand</h3>
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <div class="dashboard_breadcam text-end">
                                        <p><a href="index-2.html">Dashboard</a> <i class="fas fa-caret-right"></i>Add
                                            Brand</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-12">
                        <div class="white_box mb_30">
                            <div class="row justify-content-center">
                                <div id="checkResult"></div>
                                <div class="col-lg-6">
                                    <div class="modal-content cs_modal">
                                        <div class="modal-body">
                                            <form method="post" action="/admin/brand/addBrand">
                                                <div class="">
                                                    <input type="text" name="brandName" id="brandName"
                                                        onkeyup="checkCategory()" class="form-control"
                                                        placeholder="Brand Name" required>
                                                </div>
                                                <button type="submit" id="submitButton"
                                                    class="btn_1 full_width text-center"> Add</button>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- footer part -->
        <%- include('../../partials/admin/adminFooter') %>
    </section>
    <!-- checking already exist -->
    <script src="../../../admin/javascript/existCheck.js"></script>
    <!-- js files included  -->
    <%- include('../../partials/admin/adminJs') %>
</body>
</html>